{
  "name": "project-vision",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.29",
    "@types/react-dom": "^17.0.1",
    "concurrently": "^5.3.0",
    "cra-template-electron-react-typescript": "2.1.1",
    "electron": "^11.2.3",
    "electron-is-dev": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.2",
    "sass": "^1.32.8",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.3",
    "twin.macro": "^2.2.3",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1"
  },
  "scripts": {
    "start": "concurrently \"yarn react-serve\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "yarn react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "yarn react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "set BROWSER=none&&yarn react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [],
  "main": "public/electron-main.js",
  "homepage": "./",
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.7"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "styled": {
        "import": "default",
        "from": "styled-components"
      },
      "css": {
        "import": "css",
        "from": "styled-components"
      }
    }
  }
}
